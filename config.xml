<?xml version='1.0' encoding='utf-8'?>
<widget id="com.wasu.emf" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>EMF INS</name>
    <description>
        EMF Instruments Book
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
       KPGSTeam
    </author>
    <content src="www/index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="landscape" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#ee6e73" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <plugin name="cordova-plugin-statusbar" source="npm" spec="~2.1.3" />
    <plugin name="cordova-plugin-console" source="npm" spec="~1.0.3" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.2.1" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />

        <icon src="EMF INstruments L1-10 Android.hyperesources/icon.png" />

        
        
        
    
   
    
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
